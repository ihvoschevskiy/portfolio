{
  "name": "env_bundle",
  "version": "1.0.0",
  "private": true,
  "description": "Окружение для react-typescript-postcss",
  "keywords": [
    "environment"
  ],
  "license": "ISC",
  "author": "Igor` Hvoschevskiy",
  "scripts": {
    "start": "cross-env-shell NODE_ENV=development node ./webpack/development.js",
    "build": "cross-env-shell NODE_ENV=production node ./webpack/production.js",
    "serve": "serve ./build",
    "analyze": "webpack-bundle-analyzer ./build/stats.json",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "lint": "eslint ./ --ext .ts,.tsx,.js,.jsx && stylelint \"./src/**/*.?(s)css\"",
    "lint:fix": "eslint ./ --ext .ts,.tsx,.js,.jsx --fix && stylelint \"./src/**/*.?(s)css\" --fix",
    "prettier": "prettier --write ./",
    "prettier:check": "prettier --check ./",
    "add": "bash ./.scripts/add.sh",
    "rm": "bash ./.scripts/rm_gitkeep.sh"
  },
  "pre-commit": [
    "lint",
    "prettier:check"
  ],
  "dependencies": {
    "install": "^0.13.0",
    "npm": "^9.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-router-dom": "^6.14.2",
    "react-transition-group": "^4.4.5"
  },
